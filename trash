def sendUpletPoint(uplet, s):

    upletX,upletY = splitXY(uplet)

    upletX = [str(i) for i in upletX]
    upletY = [str(i) for i in upletY]

    data = json.dumps(upletX).encode()
    length = len(data)
    print("Lenght of X", length)
    s.sendall(str(length).encode())
    s.recv(1)
    s.sendall(data)

    data = json.dumps(upletY).encode()
    length = len(data)
    print("Lenght of Y", length)

    s.recv(1)
    s.sendall(str(length).encode())
    s.recv(1)

    s.sendall(data)
    ok = s.recv(1)
    print("It worked", ok)
